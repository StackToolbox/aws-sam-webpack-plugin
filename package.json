  "dependencies": {
    "js-yaml": "^4.1.0",
    "yaml-cfn": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^14.17.6",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "jest": "^27.0.6",
